{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "antigravity",
  "title": "Antigravity",
  "description": "Open files and projects in Google Antigravity IDE",
  "icon": "icon.png",
  "author": "Carl_NotANerd",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-recent-projects",
      "title": "Search Recent Projects",
      "subtitle": "Antigravity",
      "description": "Search and open recent projects",
      "mode": "view"
    },
    {
      "name": "open-project",
      "title": "Open Project",
      "subtitle": "Antigravity",
      "description": "Open a project in Antigravity",
      "mode": "view"
    },
    {
      "name": "open-file",
      "title": "Open File",
      "subtitle": "Antigravity",
      "description": "Open a file in Antigravity",
      "mode": "view"
    },
    {
      "name": "open-new-window",
      "title": "Open New Window",
      "subtitle": "Antigravity",
      "description": "Open a new Antigravity window",
      "mode": "no-view"
    },
    {
      "name": "show-installed-extensions",
      "title": "Show Installed Extensions",
      "subtitle": "Antigravity",
      "description": "List installed Antigravity extensions",
      "mode": "view"
    },
    {
      "name": "open-with-antigravity",
      "title": "Open with Antigravity",
      "subtitle": "Antigravity",
      "description": "Open selected Finder items in Antigravity",
      "mode": "no-view"
    }
  ],
  "preferences": [
    {
      "name": "antigravityPath",
      "type": "textfield",
      "required": false,
      "title": "Antigravity CLI Path",
      "description": "Path to the Antigravity CLI binary",
      "default": "/Applications/Antigravity.app/Contents/Resources/app/bin/antigravity"
    },
    {
      "name": "projectRoot",
      "type": "directory",
      "required": true,
      "title": "Project Root",
      "description": "Directory containing your projects",
      "default": "~/Documents"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.84.0",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "^19.2.6",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\\"prepublishOnly\\\" script and rerun \\\"npm publish\\\" again.\\nIf you wanted to publish it to the Raycast Store instead, use \\\"npm run publish\\\" instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
